<p id="notice"><%= notice %></p>

<h1>Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Credencial cliente</th>
      <th>Empresa cliente</th>
      <th>Responsable cliente</th>
      <th>Telefono cliente</th>
      <th>Email cliente</th>
      <th>Comisionista cliente</th>
      <th>Porcentage cliente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.credencial_cliente %></td>
        <td><%= cliente.empresa_cliente %></td>
        <td><%= cliente.responsable_cliente %></td>
        <td><%= cliente.telefono_cliente %></td>
        <td><%= cliente.email_cliente %></td>
        <td><%= cliente.comisionista_cliente %></td>
        <td><%= cliente.porcentage_cliente %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
